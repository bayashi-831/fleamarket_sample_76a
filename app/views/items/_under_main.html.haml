%section.pickupContainer
  %h2.head
    ピックアップカテゴリー
  .productBox
    .productHead
      =link_to "#",class: "pickup" do
        %h3.title
          新規投稿商品
    .productLists
      .productList
        =link_to "#" ,class: "itemsphoto" do
          %figure.productList__img
            =image_tag "manga-niku.png"
          .productList__body
            %h3.name
              商品名
            .details
              %ul
                %li
                  1000円
                %li
                  = fa_icon "star"
                  %i
                    1
              %p
